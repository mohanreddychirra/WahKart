{
  "name": "wah-kart",
  "version": "1.0.0",
  "description": "E-commerce store using React.js",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV='production' && node ./build/index.js",
    "cleardb": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "postinstall": "npm run build && ./node_modules/.bin/sequelize db:migrate:undo:all || true && ./node_modules/.bin/sequelize db:migrate && ./node_modules/.bin/sequelize db:seed:all",
    "start:dev:server": "export NODE_ENV='development' && nodemon --require @babel/register server/index.js",
    "start:dev:client": "export NODE_ENV='development' && ./node_modules/.bin/webpack-dev-server",
    "build:client": "rm -rf ./dist || true && ./node_modules/.bin/webpack-cli --config webpack.prod.config.js",
    "build:server": "rm -rf ./build || true && ./node_modules/.bin/babel server/ --out-dir build/",
    "build": "npm run build:client && npm run build:server",
    "test": "npm run test:client && npm run test:server",
    "test:client": "./node_modules/.bin/jest",
    "test:server": "./node_modules/.bin/mocha --require @babel/register server/specs/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohanreddychirra/ReddyStore.git"
  },
  "author": "Sai Mohan Reddy Chirra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mohanreddychirra/ReddyStore/issues"
  },
  "homepage": "https://github.com/mohanreddychirra/ReddyStore#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "jest": "^24.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sequelize-cli": "^5.4.0",
    "style-loader": "^0.23.1",
    "supertest": "^3.4.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "file-loader": "^2.0.0",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.3.0",
    "multer": "^1.4.1",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-paginate": "^6.3.0",
    "react-rating": "^1.6.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.39.1",
    "toastr": "^2.1.4"
  }
}
